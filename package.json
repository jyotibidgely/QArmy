{
  "name": "qarmy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "axe-core": "^4.4.2",
    "cypress-axe": "^0.14.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "axe-core": "^4.4.2",
    "cypress": "^10.3.1",
    "cypress-axe": "^0.14.0",
    "cypress-downloadfile": "^1.2.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.6.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  },
  "scripts": {
    "cy:open:qa": "cypress open --env configFile=qa",
    "cy:run:qa": "cypress run --env configFile=qa",
    "cy:run:chrome": "cypress run --spec \"cypress/e2e/Ameren/GbDownloadAMIE.cy.js\",\"cypress/e2e/Ameren/GbDownloadAMRE.cy.js\" --headless --browser chrome",
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports",
    "scripts": "npm run cy:run:chrome",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test" : "npm run scripts || npm run posttest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jyotibidgely/QArmy.git"
  },
